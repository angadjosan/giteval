# GitHub API Configuration
GITHUB_TOKEN=your_github_personal_access_token_here
GITHUB_API_URL=https://api.github.com

# Claude API Configuration
CLAUDE_API_KEY=your_claude_api_key_here
CLAUDE_MODEL=claude-opus-4-5-20251101

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Redis Cache Configuration
REDIS_URL=redis://localhost:6379
# Or for production: redis://username:password@host:port

# Google Cloud Storage (for cloned repositories)
GCS_BUCKET=giteval-repos
GCS_PROJECT_ID=your_gcp_project_id

# Application Configuration
NODE_ENV=development
BASE_URL=http://localhost:3000

# Security
SESSION_SECRET=your_session_secret_here

# Rate Limiting
RATE_LIMIT_ANONYMOUS=100
RATE_LIMIT_AUTHENTICATED=500

# Repository Analysis Limits
MAX_REPO_SIZE_MB=1024
MAX_REPO_FILES=10000
ANALYSIS_TIMEOUT_MS=300000
